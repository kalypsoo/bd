@model IEnumerable<ProjetBD.Models.Examan>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.libelle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.periodicite)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numCompte)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prixSoumis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prixNonSoumis)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(model => item.libelle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.periodicite)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numCompte)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prixSoumis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prixNonSoumis)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.libelle }) |
            @Html.ActionLink("Details", "Details", new { id=item.libelle }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.libelle })
        </td>
    </tr>
}

</table>
